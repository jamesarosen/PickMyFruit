{
	"name": "@pickmyfruit/www",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"description": "www.pickmyfruit.com Application",
	"scripts": {
		"build": "vite build",
		"dev": "PORT=${PORT:-5173} vite dev",
		"start": "node .output/server/index.mjs",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"lint:fix": "oxlint --fix",
		"lint": "oxlint",
		"preview": "vite preview",
		"test": "vitest",
		"test:run": "vitest run",
		"typecheck": "tsc --noEmit",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:seed": "tsx src/data/seed.ts",
		"deploy": "fly deploy",
		"deploy:setup": "fly launch --no-deploy",
		"logs": "fly logs",
		"ssh": "fly ssh console"
	},
	"dependencies": {
		"@libsql/client": "^0.15.15",
		"@tanstack/solid-router": "^1.144.0",
		"@tanstack/solid-start": "^1.145.3",
		"better-auth": "^1.4.10",
		"drizzle-orm": "^0.45.1",
		"h3-js": "^4.4.0",
		"resend": "^6.6.0",
		"solid-js": "^1.9.5",
		"zod": "^4.3.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^10.1.0",
		"@solidjs/testing-library": "^0.8.10",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.3",
		"drizzle-kit": "^0.31.8",
		"jsdom": "^27.4.0",
		"nitro": "3.0.1-alpha.1",
		"oxlint": "^1.36.0",
		"prettier": "^3.3.3",
		"tsx": "^4.21.0",
		"typescript": "^5.6.3",
		"vite": "^7.3.0",
		"vite-plugin-solid": "^2.11.0",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.16"
	}
}
