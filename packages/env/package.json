{
  "name": "@pickmyfruit/env",
  "type": "module",
  "version": "1.0.0",
  "dependencies": {
    "dotenv": "^16.4.7",
    "valibot": "1.0.0-beta.9"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "*"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "pull": ": ${NODE_ENV:=development} && op inject -i .env.tpl -o .env.${NODE_ENV}",
    "bootstrap": "for file in .env.*.sample; do cp -n \"$file\" \"${file%.sample}\" && echo \"${file%.sample}: copied\" || echo \"${file%.sample}: exists\"; done"
  }
}
